{
  "name": "@tagsamurai/tsv2-mfe-boilerplate-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve:standalone": "vue-cli-service serve --mode standalone",
    "serve:https": "vue-cli-service serve --https true",
    "tailwind": "tailwind -i ./src/assets/css/main.css -o ./cypress/support/tailwind.css",
    "check-coverage": "lcov-total ./coverage/lcov.info",
    "cy:run-ct": "yarn tailwind && cypress run --component --headed",
    "cy:open-ct": "yarn tailwind && cypress open --component",
    "cy:run-e2e": "yarn tailwind && cypress run",
    "cy:open-e2e": "yarn tailwind && cypress open",
    "cy:test-all": "yarn tailwind && start-server-and-test dev http://localhost:9090 cy:run-e2e && yarn cy:run-ct"
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "autoprefixer": "^10.4.17",
    "axios": "^1.4.0",
    "core-js": "^3.8.3",
    "postcss": "^8.4.35",
    "single-spa-vue": "^3.0.0",
    "systemjs-webpack-interop": "^2.3.7",
    "tailwindcss": "^3.4.1",
    "tsv2-library": "^0.0.69",
    "vue": "^3.4.21",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.28",
    "@cypress/vue": "^6.0.0",
    "@types/mocha": "^10.0.6",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cypress": "^13.7.0",
    "cypress-visual-regression": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "latest",
    "eslint-config-ts-code-standard": "^1.0.9",
    "eslint-plugin-css": "^0.8.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-vue": "^9.19.2",
    "istanbul-lib-coverage": "^3.2.2",
    "lcov-total": "^2.1.1",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "5.1.6",
    "vite": "^5.2.3",
    "vite-plugin-istanbul": "^5.0.0",
    "vue-cli-plugin-single-spa": "~3.3.0",
    "webpack": "^5.90.3"
  }
}
